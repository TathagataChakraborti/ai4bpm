{
  "name": "ai4bpm",
  "version": "0.0.0",
  "repository": "git@github.com:Tathagata-Chakraborti/ai4bpm.git",
  "bugs": "https://github.com/Tathagata-Chakraborti/ai4bpm/issues",
  "license": "MIT",
  "scripts": {
    "build": "react-scripts --max_old_space_size=4096 build",
    "ci-check": "yarn format:diff",
    "clean": "yarn cache clean && yarn install",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{js,md,scss}\"",
    "format:diff": "prettier --list-different \"**/*.{js,md,scss}\"",
    "format:staged": "prettier --write",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "dependencies": {
    "d3": "^7.6.0",
    "@carbon/grid": "^10.14.0",
    "@carbon/icons-react": "^10.15.0",
    "@carbon/charts-react": "^1.6.0",
    "carbon-components": "^10.17.0",
    "carbon-components-react": "^7.17.0",
    "carbon-icons": "^7.0.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-router-hash-link": "^1.2.2",
    "react-scripts": "2.1.8",
    "copy-to-clipboard": "3.3.1",
    "react-twitter-widgets": "1.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.13.1",
    "prettier": "^1.17.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "yarn format:staged",
      "git add"
    ],
    "*.{scss,css}": [
      "yarn format:staged",
      "git add"
    ],
    "*.md": [
      "yarn format:staged",
      "git add"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
